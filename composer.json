{
    "name": "myabakus/teamworkpm",
    "description": "PHP wrapper for Teamwork.com API",
    "license": "MIT",
    "authors": [
        {
            "name": "Loduis Madariaga",
            "email": "loduis@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "TeamWorkPm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TeamWorkPm\\Tests\\": "tests/"
        },
        "files": [
            "tests/functions.php"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.6-dev"
        }
    }
}
