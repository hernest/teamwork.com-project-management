{
    "name": "myabakus/teamworkpm",
    "description": "PHP wrapper for Teamwork.com API",
    "license": "MIT",
    "authors": [
        {
            "name": "Loduis Madariaga",
            "email": "loduis@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "TeamWorkPm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TeamWorkPm\\Tests\\": "tests/"
        },
        "files": [
            "tests/functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.0-dev"
        }
    },
    "config": {
        "platform-check": true
    }
}
